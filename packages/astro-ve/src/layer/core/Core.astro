---
//?     [CORE]
export interface Props {
    widget: string;
    data?: string;
}
const { widget, data } = Astro.props;
let mod = widget.split(':')[1];
import Root from './root/Root.astro';
---
{
    mod == 'root' && (
        <Root widget={widget} data={data}>
            <slot name="head" slot="head"/>
			<slot name="nav" slot="nav"/>
			<slot name="footer" slot="footer"/>
			<slot name="raw" slot="raw"/>
			<slot />
        </Root>
    )
}
