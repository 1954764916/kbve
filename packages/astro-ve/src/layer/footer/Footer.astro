---
export interface Props {
    widget: string;
    data?: string;
}
const { data , widget } = Astro.props;
let mod = widget.split(':')[1];

import Float from './float/FloatFooter.astro';

---
{
    mod == 'float' && (
        <Float widget={widget} data={data}>
            <slot name="raw" slot="raw" />
			<slot />
        </Float>
    )

}