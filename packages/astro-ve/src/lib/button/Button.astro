---
//*         [BUTTON]
export interface Props {
	widget: string;
	data?: string;
}
const { data, widget } = Astro.props;
let mod = widget.split(':')[1]
import Shutter from './shutter/Shutter.astro';
---

{
	mod == 'shutter' && (
		<Shutter widget={widget} data={data}>
			<slot name="raw" slot="raw" />
			<slot />
		</Shutter>
	)
}
